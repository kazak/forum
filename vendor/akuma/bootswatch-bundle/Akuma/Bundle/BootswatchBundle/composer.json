{
    "name"             : "akuma/bootswatch-bundle",
    "type"             : "symfony-bundle",
    "description"      : "Akuma Bootswatch Bundle",
    "keywords"         : ["bootstrap", "bootswatch"],
    "homepage"         : "",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Nikita Makarov",
